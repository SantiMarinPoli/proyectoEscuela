@*
@model List<ObjetoEscuelaBase>
*@

<table class="table table-dark table-hover table-sm">
    <thead>
        <tr>
            <th class="col">Nombre</th>
            <th class="col">ID</th>
        </tr>
    </thead>
    <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td>@obj.Nombre</td>
                    <td>@obj.UniqueId</td>
                </tr>
            }
    </tbody>
</table>